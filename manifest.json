{
  "name": "pchome Support",
  "description": "買買輔助程式",
  "version": "1.4.2",
  "permissions": [ "activeTab", "storage" ],
  "background": {
    "scripts": [
      "browser-polyfill.min.js",
      "background.js" 
    ]
  },
  "browser_action": {
    "default_title": "買買輔助程式"
  },
  "options_page": "options.html",
  "content_scripts" : [
    {
      "matches" : [ "https://24h.pchome.com.tw/prod/*" ],
      "run_at": "document_end",
      "js" : [ "jquery.min.js", "js/prod.js" ]
    }
  ],
  "manifest_version": 2
}
